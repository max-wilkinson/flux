{
  "name": "flux",
  "displayName": "Flux",
  "description": "A flexible timer for establishing your working rhythm",
  "license": "SEE LICENSE IN LICENSE",
  "keywords": [
    "timer",
    "rhythm",
    "pomodoro",
    "desktime",
    "ultradian"
  ],
  "icon": "images/FluxIcon.png",
  "galleryBanner": {
    "color": "#7E66C2",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/max-wilkinson/flux"
  },
  "publisher": "FordLabs",
  "preview": true,
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.flux"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.flux",
        "title": "Flux"
      },
      {
        "command": "extension.resetFlux",
        "title": "Reset Flux"
      }
    ],
    "configuration": {
      "title": "Flux",
      "properties": {
        "flux.rhythm": {
          "type": "string",
          "description": "Select a rhythm for your flux timer.",
          "enumDescriptions": [
            "Pomodoro (25 minutes)",
            "Desktime (52 minutes)",
            "Ultradian (90 minutes)"
          ],
          "enum": [
            "Pomodoro",
            "Desktime",
            "Ultradian"
          ]
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.40.0",
    "glob": "^7.1.5",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2"
  },
  "dependencies": {
    "moment": "^2.29.2"
  }
}
